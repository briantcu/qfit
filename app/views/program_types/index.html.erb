<h1>Listing program_types</h1>

<table>
  <thead>
    <tr>
      <th>Program type</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @program_types.each do |program_type| %>
      <tr>
        <td><%= program_type.program_type %></td>
        <td><%= link_to 'Show', program_type %></td>
        <td><%= link_to 'Edit', edit_program_type_path(program_type) %></td>
        <td><%= link_to 'Destroy', program_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

